<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Fetchify_Fetchify::main_config" title="Address Auto-Complete"/>
                            <resource id="Fetchify_Fetchify::pcl_config" title="Postcode Lookup" />
                            <resource id="Fetchify_Fetchify::email_config" title="Email Validation" />
                            <resource id="Fetchify_Fetchify::phone_config" title="Phone Validation" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>

