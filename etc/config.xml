<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<fetchify_main>
			<main_options>
				<enable_extension>1</enable_extension>
				<accesstoken backend_model="Magento\Config\Model\Config\Backend\Encrypted"></accesstoken>
			</main_options>
		</fetchify_main>
		<fetchify_email>
			<options>
				<enable>0</enable>
			</options>
		</fetchify_email>
		<fetchify_phone>
			<options>
				<enable>0</enable>
			</options>
		</fetchify_phone>
		<fetchify_pcl>
			<options>
				<enable>0</enable>
			</options>
			<gfx_options>
				<hide_fields>0</hide_fields>
			</gfx_options>
			<txt_options>
				<search_placeholder>Type your postcode here</search_placeholder>
				<search_buttontext>Find Address</search_buttontext>
				<manual_entry>Enter Address Manually</manual_entry>
				<search_label>Postcode Lookup</search_label>
				<error_msg_1>This postcode could not be found, please try again or enter your address manually.</error_msg_1>
				<error_msg_2>This postcode is not valid.</error_msg_2>
				<error_msg_3>Unable to connect to address lookup server.</error_msg_3>
				<error_msg_4>An unexpected error occured.</error_msg_4>
			</txt_options>
			<advanced>
				<county_data>none</county_data>
			</advanced>
		</fetchify_pcl>
		<fetchify_global>
			<main_options>
				<frontend_enabled>0</frontend_enabled>
				<accesstoken backend_model="Magento\Config\Model\Config\Backend\Encrypted">xxxxx-xxxxx-xxxxx-xxxxx</accesstoken>
			</main_options>
			<gfx_options>
				<mode>1</mode>
				<ambient>light</ambient>
				<accent>default</accent>
			</gfx_options>
			<txt_options>
				<search_label>Address Search</search_label>
				<search_placeholder>Start with post/zip code or street</search_placeholder>
				<country_placeholder>Type here to search for a country</country_placeholder>
				<country_button>Change Country</country_button>
				<error_msg_1>No results found</error_msg_1>
				<error_msg_2>An error occurred. Please enter your address manually</error_msg_2>
				<manual_entry_toggle>Enter address manually</manual_entry_toggle>
			</txt_options>
			<advanced>
				<transliterate>0</transliterate>
				<hide_fields>0</hide_fields>
				<lock_country_to_dropdown>0</lock_country_to_dropdown>
				<search_elem_id></search_elem_id>
				<use_first_line>0</use_first_line>
				<debug>0</debug>
				<fill_uk_counties>1</fill_uk_counties>
			</advanced>
			<exclusions>
				<areas></areas>
				<po_box>0</po_box>
			</exclusions>
		</fetchify_global>
	</default>
</config>
